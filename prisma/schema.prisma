generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       String @id @default(uuid())
  username String @unique
  email    String
  password String
  name String 

  createdAt DateTime @default(now())
}
